{
    "nixEnvSelector.nixFile": "${workspaceRoot}/flake.nix",
    "deno.enable": true,
    "deno.lint": true,
    "deno.unstable": false,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "**/.direnv/flake-*": true,
    }
}