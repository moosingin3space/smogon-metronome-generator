{
    "nixEnvSelector.nixFile": "${workspaceRoot}/flake.nix",
    "deno.enable": true,
    "deno.lint": true,
    "deno.unstable": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "**/.direnv/*": true,
        "**/dist/*": true,
    },
    "deno.suggest.imports.hosts": {
        "https://deno.land": true,
        "https://cdn.skypack.dev": false
    }
}